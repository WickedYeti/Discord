{
    "name": "discord/framework",
    "type": "library",
    "description": "Non-frustrating Experimental PHP5.6 Framework",
    "homepage": "https://github.com/wickedyeti/discord",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymeric Assier",
            "email": "aymeric.assier@gmail.com",
            "homepage": "http://www.assier.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Discord\\": "/"
        },
        "files": [
            "/Box/helpers/arrays.php",
            "/Box/helpers/filters.php",
            "/Box/helpers/strings.php",
            "/Persist/helpers.php",
            "/Debug/helpers.php"
        ]
    },
    "replace": {
        "discord/box": "self.version",
        "discord/debug": "self.version",
        "discord/event": "self.version",
        "discord/http": "self.version",
        "discord/persist": "self.version",
        "discord/reflector": "self.version",
        "discord/router": "self.version",
        "discord/view": "self.version",
        "discord/orm": "self.version",
        "discord/web": "self.version"
    },
    "require": {
        "psr/log": "~1.0"
    }
}